  PathToAcad, Blad, Blad1: string;
  AllProfilesAcad: TArrayOfString;
  ProfileBezBest: TArrayOfString;
begin

  //regAutoCAD1 = 'SOFTWARE\GSTARSOFT\GSTARCAD\';
