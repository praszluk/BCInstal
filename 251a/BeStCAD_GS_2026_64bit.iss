  //wyciagniecie danych z rejestru na poziomie 2026
  RegGetSubkeyNames(HKEY_LOCAL_MACHINE, regAutoCAD, tabAcadMainRegs1);
  //wyrzucenie z tablicy GSTARCADów innych niz 2026
  stringWersja := 'R26';
