  //liczymy ile w danym autocadzie jest profili, nie liczac profili bestcad
  k := 0;
  for i:=0 to ( GetArrayLength(AllProfilesAcad) - 1 ) do
  begin
    if (Length(AllProfilesAcad[i]) = 9) then
    begin
      if (CzyNieZawieraBest(AllProfilesAcad[i])) then
      begin
        k := k + 1;
      end;
    end else
    begin
      k := k + 1;
    end;
  end;
  
  if k = 0 then
